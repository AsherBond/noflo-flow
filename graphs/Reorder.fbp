EXPORT=INLEFT.IN:LEFTIN
EXPORT=CACHE.OUT:LEFTOUT
EXPORT=MERGEINPUT.IN:RIGHTIN
EXPORT=SPLITINPUT.OUT:RIGHTOUT

# Left must come in before right as left is cached
MergeInput(Merge) OUT -> IN SplitInput(Split) OUT -> READY Cache(flow/Cache)

# Invalidate cache after a certain count of connections, by default 1
SplitInput() OUT -> IN CountDown(flow/CountDown) OUT -> IN InLeft(Merge) OUT -> IN Cache()
